{
  "name": "sap-ewm-stock-lookup",
  "version": "1.0.0",
  "description": "SAP BTP CAP Application for EWM Warehouse Physical Stock Lookup",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@sap/cds-dk": "^9"
  },
  "scripts": {
    "start": "cds-serve",
    "watch": "cds watch",
    "build": "cds build --production"
  },
  "cds": {
    "requires": {
      "EWM_HMF": {
        "kind": "rest",
        "credentials": {
          "destination": "EWM_HMF"
        }
      }
    }
  },
  "sapux": [
    "app/stock-lookup"
  ]
}
