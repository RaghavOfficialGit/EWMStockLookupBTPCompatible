{
  "name": "sap-ewm-stock-lookup",
  "version": "1.0.0",
  "description": "SAP BTP CAP Application for EWM Warehouse Physical Stock Lookup",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@sap/cds": "^9",
    "@sap-cloud-sdk/http-client": "^3",
    "@sap-cloud-sdk/connectivity": "^3",
    "@sap-cloud-sdk/resilience": "^3",
    "express": "^4"
  },
  "devDependencies": {
    "@sap/cds-dk": "^9"
  },
  "scripts": {
    "start": "cds-serve",
    "watch": "cds watch --profile production",
    "build": "cds build --production"
  },
  "cds": {
    "requires": {
      "db": false,
      "EWM_HMF": {
        "kind": "rest",
        "credentials": {
          "destination": "EWM_HMF"
        }
      }
    },
    "[development]": {
      "requires": {
        "db": false
      }
    },
    "[production]": {
      "requires": {
        "db": false
      }
    }
  },
  "sapux": [
    "app/stock-lookup"
  ]
}
