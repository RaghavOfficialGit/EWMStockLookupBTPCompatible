{
  "_version": "1.59.0",
  "sap.app": {
    "id": "ewm.stock.lookup",
    "type": "application",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "mainService": {
        "uri": "/stock/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0",
          "localUri": "localService/metadata.xml"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "stockLookup-display": {
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          },
          "semanticObject": "WarehouseStock",
          "action": "lookup",
          "title": "{{appTitle}}",
          "icon": "sap-icon://inventory"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://inventory",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.fe.templates": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ewm.stock.lookup.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "WarehousePhysicalStockList",
          "target": "WarehousePhysicalStockList"
        },
        {
          "pattern": "WarehousePhysicalStock({key}):?query:",
          "name": "WarehousePhysicalStockObjectPage",
          "target": "WarehousePhysicalStockObjectPage"
        }
      ],
      "targets": {
        "WarehousePhysicalStockList": {
          "type": "Component",
          "id": "WarehousePhysicalStockList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/WarehousePhysicalStock",
              "variantManagement": "Page",
              "navigation": {
                "WarehousePhysicalStock": {
                  "detail": {
                    "route": "WarehousePhysicalStockObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable",
                    "selectionMode": "None",
                    "enableExport": true,
                    "enableFullScreen": true,
                    "enablePaste": false,
                    "condensedTableLayout": true,
                    "personalization": {
                      "column": true,
                      "filter": true,
                      "sort": true
                    }
                  },
                  "columns": {
                    "DataField::Product": {
                      "width": "10rem"
                    },
                    "DataField::EWMWarehouse": {
                      "width": "6rem"
                    },
                    "DataField::EWMStockType": {
                      "width": "6rem"
                    },
                    "DataField::EWMStorageBin": {
                      "width": "10rem"
                    },
                    "DataField::EWMStockQuantityInBaseUnit": {
                      "width": "8rem"
                    },
                    "DataField::EWMStockQuantityBaseUnit": {
                      "width": "5rem"
                    }
                  }
                },
                "@com.sap.vocabularies.UI.v1.SelectionFields": {
                  "filterFields": {
                    "Product": {
                      "label": "Product",
                      "settings": {
                        "operatorConfiguration": [
                          {
                            "path": "key",
                            "equals": "EQ",
                            "exclude": false
                          }
                        ]
                      }
                    },
                    "EWMStockType": {
                      "label": "Stock Type",
                      "settings": {
                        "operatorConfiguration": [
                          {
                            "path": "key",
                            "equals": "EQ",
                            "exclude": false
                          }
                        ]
                      }
                    },
                    "Batch": {
                      "label": "Batch",
                      "settings": {
                        "operatorConfiguration": [
                          {
                            "path": "key",
                            "equals": "EQ",
                            "exclude": false
                          }
                        ]
                      }
                    },
                    "HandlingUnitNumber": {
                      "label": "Handling Unit",
                      "settings": {
                        "operatorConfiguration": [
                          {
                            "path": "key",
                            "equals": "EQ",
                            "exclude": false
                          }
                        ]
                      }
                    },
                    "EWMStorageBin": {
                      "label": "Storage Bin",
                      "settings": {
                        "operatorConfiguration": [
                          {
                            "path": "key",
                            "equals": "EQ",
                            "exclude": false
                          }
                        ]
                      }
                    }
                  },
                  "layout": "CompactFilters",
                  "initialLayout": "Visual",
                  "useSemanticDateRange": false
                }
              },
              "initialLoad": "Disabled",
              "views": {
                "paths": [
                  {
                    "primary": [
                      {
                        "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant"
                      }
                    ],
                    "secondary": [
                      {
                        "annotationPath": "com.sap.vocabularies.UI.v1.LineItem"
                      }
                    ],
                    "defaultPath": "both"
                  }
                ]
              }
            }
          }
        },
        "WarehousePhysicalStockObjectPage": {
          "type": "Component",
          "id": "WarehousePhysicalStockObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/WarehousePhysicalStock"
            }
          }
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.cloud": {
    "public": true,
    "service": "sap-ewm-stock-lookup"
  }
}
